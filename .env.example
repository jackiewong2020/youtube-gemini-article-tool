# Gemini
GEMINI_API_KEY=your_gemini_api_key
GEMINI_MODEL=gemini-2.5-pro
GEMINI_IMAGE_MODEL=gemini-2.5-flash-image

# Alibaba OSS
OSS_ACCESS_KEY_ID=your_access_key_id
OSS_ACCESS_KEY_SECRET=your_access_key_secret
OSS_ENDPOINT=oss-cn-shenzhen.aliyuncs.com
OSS_BUCKET_NAME=your_bucket_name
OSS_DOMAIN=https://your_bucket_name.oss-cn-shenzhen.aliyuncs.com

# Optional: when style is configured in OSS image processing
OSS_STYLE=wechat-style

# Optional: protect the Streamlit page with a password
APP_PASSWORD=your_dashboard_password
